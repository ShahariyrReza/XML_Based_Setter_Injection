<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="coder1" class="com.example.model.coder" >
	<property name="id" value="101"></property>
	<property name="name" value="Shahariyr Reza"></property>
	<property name="language" value="Java"></property>
	<property name="computer" ref="computer1"></property>
	
	</bean>
	
	<bean id="computer1" class="com.example.model.computer">
	
	<property name="brand" value="HP"></property>
	
	</bean>
	
	
</beans>
